      "title": "Shadow Testing - Legacy vs New Routing Engine",
      "type": "row",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "collapsed": false
    },
    {
      "title": "Response Time Comparison",
      "type": "graph",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, routing_duration_seconds_bucket{system=\"new_routing_engine\"})",
          "legendFormat": "New Engine P95",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, routing_duration_seconds_bucket{system=\"legacy_system\"})",
          "legendFormat": "Legacy System P95",
          "refId": "B"
        },
        {
          "expr": "45",
          "legendFormat": "Shadow SLA (45ms)",
          "refId": "C"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 1
      },
      "id": 2,
      "yAxes": [
        {
          "label": "Milliseconds",
          "min": 0,
          "max": 100
        },
        {
          "show": false
        }
      ],
      "thresholds": [
        {
          "value": 45,
          "colorMode": "critical",
          "op": "gt",
          "fill": true,
          "line": true
        }
      ]
    },
    {
      "title": "Success Rate Comparison",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(routing_requests_total{status=\"success\",system=\"new_routing_engine\"}[5m]) / rate(routing_requests_total{system=\"new_routing_engine\"}[5m]) * 100",
          "legendFormat": "New Engine Success Rate",
          "refId": "A"
        },
        {
          "expr": "rate(routing_requests_total{status=\"success\",system=\"legacy_system\"}[5m]) / rate(routing_requests_total{system=\"legacy_system\"}[5m]) * 100",
          "legendFormat": "Legacy System Success Rate",
          "refId": "B"
        },
        {
          "expr": "98.5",
          "legendFormat": "Minimum SLA (98.5%)",
          "refId": "C"
        }
