  "panels": [
    {
      "title": "Routing Engine Performance Overview",
      "type": "row",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "collapsed": false
    },
    {
      "title": "Request Rate",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(routing_requests_total[5m])",
          "legendFormat": "{{method}} - {{status}}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 1
      },
      "id": 2,
      "yAxes": [
        {
          "label": "Requests/sec",
          "min": 0
        },
        {
          "show": false
        }
      ]
    },
    {
      "title": "Response Time (P95)",
      "type": "graph",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, routing_duration_seconds_bucket)",
          "legendFormat": "P95 Response Time",
          "refId": "A"
        },
        {
          "expr": "50",
          "legendFormat": "SLA Threshold (50ms)",
          "refId": "B"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 1
      },
      "id": 3,
      "yAxes": [
        {
          "label": "Milliseconds",
          "min": 0,
          "max": 100
        },
        {
          "show": false
        }
      ],
      "thresholds": [
        {
          "value": 50,
          "colorMode": "critical",
          "op": "gt",
          "fill": true,
          "line": true
        }
      ]
    },
    {
      "title": "Success Rate",
      "type": "singlestat",
      "targets": [
        {
          "expr": "rate(routing_requests_total{status=\"success\"}[5m]) / rate(routing_requests_total[5m]) * 100",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 0,
        "y": 9
      },
      "id": 4,
      "thresholds": "98.5,99",
      "colorBackground": true,
      "format": "percent",
      "valueName": "current"
    },
    {
      "title": "Circuit Breaker Status",
      "type": "singlestat",
      "targets": [
        {
          "expr": "circuit_breaker_open",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 6,
        "y": 9
      },
      "id": 5,
      "thresholds": "0.5,0.5",
      "colorBackground": true,
      "valueMaps": [
        {
          "value": "0",
          "text": "CLOSED"
        },
        {
          "value": "1",
          "text": "OPEN"
        }
      ]
    },
    {
      "title": "Active Workers",
      "type": "singlestat",
      "targets": [
        {
          "expr": "active_workers_total",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 12,
        "y": 9
      },
      "id": 6,
      "format": "short",
      "colorBackground": false,
      "valueName": "current"
    },
    {
      "title": "Queue Depth",
      "type": "singlestat",
      "targets": [
        {
          "expr": "queue_depth_total",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 18,
        "y": 9
      },
      "id": 7,
      "format": "short",
      "thresholds": "100,200",
      "colorBackground": true,
      "valueName": "current"
    },
    {
      "title": "Routing Method Distribution",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(routing_requests_total{status=\"success\"}[5m]) by (method)",
          "legendFormat": "{{method}}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 15
      },
      "id": 8,
      "yAxes": [
        {
          "label": "Requests/sec",
          "min": 0
        },
        {
          "show": false
        }
      ]
    },
    {
      "title": "Error Rate by Type",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(routing_requests_total{status=\"error\"}[5m]) by (method)",
          "legendFormat": "{{method}} errors",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 15
      },
      "id": 9,
      "yAxes": [
        {
          "label": "Errors/sec",
          "min": 0
        },
        {
          "show": false
        }
      ]
    }
  ],
